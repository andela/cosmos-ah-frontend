{
  "name": "ah-frontend",
  "version": "0.0.1",
  "private": true,
  "description": "Frontend framework for Authors' Haven - A Social platform for the creative at heart. Built with React and Redux.",
  "testURL": "http://localhost",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint-fix",
      "git add --force"
    ],
    "*.jsx": [
      "npm run lint-fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll",
    "coverage": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development --open --hot",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "heroku-postbuild": "rimraf dist && npm run build",
    "start": "node server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@types/jest": "^24.0.12",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "nyc": "^14.0.0",
    "prettier": "^1.17.0",
    "react-dev-utils": "^9.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "jquery": "^3.4.1",
    "jwt-decode": "^2.2.0",
    "react-testing-library": "^7.0.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-tabs": "^3.0.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.86.0",
    "stringz": "^1.0.0",
    "styled-components": "^4.2.0",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^1.1.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "verbose": true,
    "testURL": "http://localhost:9001/",
    "setupFiles": [
      "<rootDir>/src/__mocks__/test-ignore-env.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/assetsTransformer.js",
      "\\.(css|less|scss)$": "<rootDir>/src/__mocks__/assetsTransformer.js"
    }
  }
}
